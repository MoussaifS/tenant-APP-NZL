# Frontend Environment Configuration Template
# Copy this file to .env.local and update the values for your environment

# Strapi Backend API URL
# This should point to your Strapi backend server
# For development: http://localhost:1337
# For production: https://your-strapi-domain.com
NEXT_PUBLIC_STRAPI_URL=http://localhost:1337

# App Configuration
NEXT_PUBLIC_APP_NAME=Tenant App NZL
NEXT_PUBLIC_APP_VERSION=1.0.0

# Development/Production Environment
NODE_ENV=development

# Optional Configuration
# API request timeout in milliseconds
# NEXT_PUBLIC_API_TIMEOUT=30000

# Enable debug mode for additional logging
# NEXT_PUBLIC_DEBUG_MODE=true

# Analytics or tracking (if needed)
# NEXT_PUBLIC_GA_TRACKING_ID=your-tracking-id

# Feature flags (if needed)
# NEXT_PUBLIC_ENABLE_FEATURE_X=true
